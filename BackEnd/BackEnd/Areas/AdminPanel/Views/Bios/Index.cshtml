@model Bio
@{
    ViewData["Title"] = "Index";
}

<table class="table table-striped table-light">
    <thead>
        <tr>
            <th>Header</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Address</th>


        </tr>
    </thead>
    <tbody>
        <tr>
            <td><img src="~/img/logo/@Model.Logo" alt="Alternate Text" /></td>
            <td>@Model.Email</td>
            <td>@Model.Phono</td>
            <td>@Html.Raw(Model.Address)</td>



        </tr>
    </tbody>
</table>





<div class="row justify-content-center mt-4">
    <a class="btn btn-dark" asp-area="AdminPanel" asp-controller="Bios" asp-action="Update" asp-route-id="@Model.Id">Update</a>
</div>

