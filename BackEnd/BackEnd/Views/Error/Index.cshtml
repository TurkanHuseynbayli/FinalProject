
@{
    ViewData["Title"] = "Index";
}

<!-- MAIN START -->
<main>
    <section id="heading">
        <div class="heading-banner-area overlay-bg"
             style="
            background-image: url(img/bg/1.jpg);
            background-position: center;
            background-repeat: no-repeat;
          ">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="heading-banner">
                            <div class="heading-banner-title">
                                <h2>404</h2>
                            </div>
                            <div class="breadcumbs pb-15">
                                <ul>
                                    <li class="home"><a asp-controller="Home" asp-action="Index" >Home</a></li>
                                    <li>404</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- 404-AREA START -->
    <section id="error-page">
        <div class="area-404 pt-80 pb-80 padding-100">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="error-content text-center">
                            <img  src="~/img/bg/error.png" alt="" />
                            <h4 class="text-light-black mt-5 mb-5">
                                Ooops.... Error 404
                            </h4>

                            <a class="button-one submit-btn-4 go-to-home bg-white text-uppercase text-light-black mt-5"
                               href="index.html"
                               data-text="go to home page">go to home page</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- 404-AREA END -->
</main>
<!-- MAIN END -->
